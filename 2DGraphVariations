import matplotlib.pyplot as plt
import numpy as np

x=np.random.rand(20)
y=np.random.rand(20)
x.sort()
y.sort()

plt.plot(x,y,'--')
x=x+.2

plt.plot(x,y,'-')
x=x+.2
plt.plot(x,y,'-.')
x=x+0.2
plt.plot(x,y,',')
x=x+.2
plt.plot(x,y,'o')
x=x+.2
plt.plot(x,y,'v')
x=x+.2
plt.plot(x,y,'p')
x=x+.2
plt.plot(x,y,'d')

plt.title('Sample 2-d Graph')
plt.ylabel('Y axis  label')
plt.xlabel('X axis label')
plt.show()

## https://matplotlib.org/api/_as_gen/matplotlib.pyplot.plot.html#matplotlib.pyplot.plot
